{
    "name": "slam/phpstan-zend-mvc",
    "description": "Zend Framework 3 MVC controller plugin extension for PHPStan.",
    "type": "phpstan-extension",
    "require": {
        "php": "^7.1",
        "zendframework/zend-mvc": "^3.1",
        "phpstan/phpstan": "^0.11.8"
    },
    "require-dev": {
        "nette/di": "^3.0",
        "nette/neon": "^3.0",
        "phpstan/phpstan-php-parser": "^0.11",
        "phpstan/phpstan-phpunit": "^0.11",
        "phpunit/phpunit": "^7.5",
        "roave/security-advisories": "dev-master",
        "slam/php-cs-fixer-extensions": "^1.19",
        "slam/php-debug-r": "^1.4",
        "zendframework/zend-mvc": "^3.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Greg Bell",
            "email": "greg@bitwombat.com.au"
        },
        {
            "name": "Michael Gooden",
            "email": "me@michaelgooden.net"
        },
        {
            "name": "Filippo Tessarotto",
            "email": "zoeslam@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "ZendPhpStan\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    }
}
